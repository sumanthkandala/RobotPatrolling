<!-- filename: hello.sumo.cfg -->
<configuration>
  <input>
      <net-file value="net.net.xml"/>
      <route-files value="hello.rou.xml"/>
      <gui-settings-file value="hello.settings.xml"/>
  </input>
  <time>
      <begin value="0"/>
      <step-length value="2.0" />
